################################################################################
# PROJECT ENVIRONMENT CONFIGURATION TEMPLATE
# ------------------------------------------------------------------------------
# Copy this file to `.env` and populate each variable with valid values.
#
# Usage:
#   cp .env.example .env
#
# Never commit your actual `.env` file to version control.
# Treat all keys and credentials as confidential.
################################################################################


# ==============================================================================
# SECTION 1: CORE LLM AND AI SERVICE CREDENTIALS
# ==============================================================================

# --- Anthropic ---
# Used for Claude models. Get your API key from: https://console.anthropic.com
ANTHROPIC_API_KEY=

# --- Google ---
# Used for Gemini / Vertex AI integrations.
# Obtain key from Google Cloud Console > API & Services > Credentials.
GOOGLE_API_KEY=

# --- Tavily ---
# Used for web search and research retrieval API.
# Documentation: https://docs.tavily.com/
TAVILY_API_KEY=


# ==============================================================================
# SECTION 2: FIRECRAWL CONFIGURATION
# ==============================================================================
# FireCrawl is a content crawling and scraping service used for structured
# web data extraction. Default API endpoint is provided below.
# Documentation: https://firecrawl.dev/docs

FIRECRAWL_API_URL=https://api.firecrawl.dev
FIRECRAWL_API_KEY=


# ==============================================================================
# SECTION 3: OPENROUTER (OPENAI-COMPATIBLE GATEWAY)
# ==============================================================================
# OpenRouter allows you to access multiple LLMs using OpenAIâ€™s API interface.
# Documentation: https://openrouter.ai/docs
#
# Replace the default base URL if using a private routing endpoint.

OPENAI_API_BASE=https://openrouter.ai/api/v1
OPENAI_API_KEY=


# ==============================================================================
# SECTION 4: FIREBASE / FIRESTORE CONFIGURATION
# ==============================================================================
# Used for persistent data storage or session tracking.
# All values can be retrieved from your Firebase Project Settings.
# Documentation: https://firebase.google.com/docs/firestore

FIRESTORE_PROJECT_ID=
FIRESTORE_SESSION_ID=
FIRESTORE_COLLECTION=


# ==============================================================================
# END OF CONFIGURATION
# ==============================================================================